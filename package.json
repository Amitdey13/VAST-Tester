{
  "name": "VAST-Tester",
  "version": "0.0.0",
  "description": "Tests IAB VAST ads.",
  "private": true,
  "repository": "InteractiveAdvertisingBureau/VAST-Tester",
  "engines": {
    "node": ">=8",
    "yarn": ">=1.7.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "homepage": "//vasttester.iab.com",
  "homepageAlt": "//vasttester2.iab.com",
  "scripts": {
    "start": "react-scripts start",
    "clean": "rimraf build",
    "build": "yarn run clean && REACT_APP_BUILD_DATE=\"$(TZ=GMT date)\" react-scripts build",
    "test": "standard \"src/**/*.js\"",
    "deploy": "yarn run build && scripts/deploy.sh",
    "postversion": "yarn run deploy"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "base16": "^1.0.0",
    "debug": "^4.1.0",
    "file-saver": "^2.0.0-rc.4",
    "font-awesome": "^4.7.0",
    "husky": "^1.1.3",
    "iab-vast-loader": "^2.0.0",
    "lint-staged": "^8.0.4",
    "lodash-es": "^4.17.11",
    "lower-case-first": "^1.0.2",
    "moment": "^2.22.2",
    "node-sass": "^4.10.0",
    "prettier-standard": "^8.0.1",
    "prop-types": "^15.6.2",
    "qs": "^6.5.2",
    "react": "^16.6.1",
    "react-collapsible": "^2.3.1",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.6.1",
    "react-fontawesome": "^1.6.1",
    "react-json-tree": "^0.11.0",
    "react-redux": "^5.1.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "react-tabs": "^2.3.0",
    "react-toggle": "^4.0.2",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.0.0",
    "reset-css": "^4.0.1",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "serialize-error": "^3.0.0",
    "standard": "^12.0.1",
    "upper-case-first": "^1.1.2",
    "url-search-params": "^1.1.0"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier-standard",
      "git add"
    ]
  },
  "standard": {
    "globals": [
      "Blob"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
